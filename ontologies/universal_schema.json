    "Agent": {
      "uri": "kg:Agent",
      "type": "owl:Class",
      "rdfs:label": [{"@value": "Agent", "@language": "en"}],
      "rdfs:comment": "Any entity capable of action: Person, Organization, AI System, Software",
      "rdfs:subClassOf": "Entity"
    },
    "Person": {
      "uri": "kg:Person",
      "type": "owl:Class",
      "rdfs:label": [{"@value": "Person", "@language": "en"}],
      "rdfs:comment": "An individual human being",
