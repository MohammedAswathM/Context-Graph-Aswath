    "Organization": {
      "uri": "kg:Organization",
      "type": "owl:Class",
      "rdfs:label": [{"@value": "Organization", "@language": "en"}],
      "rdfs:comment": "Any organized group: Company, Government, Non-profit, Institution",
      "rdfs:subClassOf": "Agent"
    },
